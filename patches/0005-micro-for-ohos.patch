+constexpr int kTempHiddenOutputIndex = 8;
+constexpr int kGateNum = 4;
+constexpr int kWeightsIndex = 3;
+const int kWeightsOrderMap[8] = {0, 2, 3, 1, 4, 6, 7, 5};  // IFGO order to IOFG order
