+REG_DYNAMIC_OPERATOR_CODER(kAllTargets, kNumberTypeInt32, PrimitiveType_StridedSlice,
+                           CPUOpCoderCreator<StridedSliceDynamicBaseCoder>)
+}  // namespace mindspore::lite::micro
